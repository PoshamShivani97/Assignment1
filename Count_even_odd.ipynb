    "Inp=tuple(map(int,input(\"Please give input separated by Spaces: \").split()))\n",
    "even_cnt=0\n",
    "odd_cnt=0\n",
    "for i in Inp:\n",
    "    if (i%2==0):\n",
    "        even_cnt+=1\n",
    "    else:\n",
    "        odd_cnt+=1\n",
    "print(\"Count of Even numbers:\",even_cnt)\n",
    "print(\"Count of Odd numbers:\",odd_cnt)\n",
    "    "
